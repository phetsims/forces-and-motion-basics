{
  "name": "forces-and-motion-basics",
  "version": "2.4.0-dev.5",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/forces-and-motion-basics.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "FORCES_AND_MOTION_BASICS",
